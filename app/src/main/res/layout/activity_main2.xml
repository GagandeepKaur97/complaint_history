<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Main2Activity">
    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true">

    <ScrollView
        android:id="@+id/scrollview"
        android:layout_width="409dp"
        android:layout_height="729dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="1dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:padding="10dp"
        tools:context= ".complaint register">

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

<TextView
    android:layout_height="wrap_content"
    android:layout_width="wrap_content"
    android:text="complaint issues"
    android:layout_marginTop="20dp"
    android:layout_marginBottom="10dp"
    android:gravity="center"/>

                <TableRow
                    android:layout_marginTop="10dp">
                    <TextView
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="suffix"/>
                    <Spinner
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:id="@+id/spinner"
                        android:layout_marginLeft="5dp"
                        android:layout_weight="1"
                        android:entries="@array/suffix"/>
                </TableRow>
                <TextView
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="First Name"/>
                <EditText
                    android:id="@+id/first_name"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:layout_marginLeft="5dp"
                    android:hint="first name"
                    android:layout_weight="1"/>

            <TableRow
                android:layout_marginTop="10dp">
                <TextView
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text=" Last Name"/>
                <EditText
                    android:id="@+id/last_name"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:layout_marginLeft="5dp"
                    android:hint="last name"
                    android:layout_weight="1"/>
            </TableRow>
            <TableRow
                android:layout_marginTop="10dp">
                <TextView
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Employment status"/>
                <Spinner
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:id="@+id/status"
                    android:layout_marginLeft="5dp"
                    android:layout_weight="1"
                    android:entries="@array/status"/>
            </TableRow>
            <TableRow
                android:layout_marginTop="10dp">
                <TextView
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Designation"/>
                <Spinner
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:id="@+id/designation"
                    android:layout_marginLeft="5dp"
                    android:layout_weight="1"
                    android:entries="@array/designation"/>
            </TableRow>
            <TableRow
                android:layout_marginTop="10dp">
                <EditText
                    android:id="@+id/stno"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:hint="street no" />
                <EditText
                    android:id="@+id/streetname"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:hint="street name"/>


            </TableRow>
            <TableRow
            android:layout_marginTop="10dp">
            <EditText
                android:id="@+id/provinance"
                android:layout_height="wrap_content"
                android:layout_width="30dp"

                android:hint="provinance"/>

                <EditText
                    android:id="@+id/city"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:hint="city" />
            </TableRow>
            <TableRow
                android:layout_marginTop="10dp">

                <EditText
                    android:id="@+id/country"
                    android:layout_width="30dp"
                    android:layout_height="wrap_content"

                    android:hint="country" />

                <EditText
                    android:id="@+id/postalcode"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"

                    android:hint="postalcode" />
            </TableRow>
            <TableRow
                android:layout_marginTop="10dp">
            <EditText
                android:id="@+id/email"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"

                android:hint="Email id"/>
            </TableRow>
            <TableRow
                android:layout_marginTop="10dp">
                <EditText
                    android:id="@+id/countrycode"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"

                    android:hint="country code"/>
                <EditText
                    android:id="@+id/cellnumber"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"

                    android:hint="cell number"/>
            </TableRow>
            <TableRow
                android:layout_marginTop="10dp">
                <TextView
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text=" issue date"/>

                <EditText
                    android:id="@+id/txtDate"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_weight="1"
                    android:hint="dd/mm/yy" />
            </TableRow>
            <TableRow
                android:layout_marginTop="10dp">
                <TextView
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="type of issue"/>
                <Spinner
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:id="@+id/issue"
                    android:layout_marginLeft="5dp"
                    android:layout_weight="1"
                    android:entries="@array/issue"/>
            </TableRow>
            <TableRow
                android:layout_marginTop="10dp">
                <TextView
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Rating bar "/>

                <RatingBar
                    android:id="@+id/rating"
                    style="@style/Widget.AppCompat.RatingBar"
                    android:isIndicator="false"
                    android:numStars="5"
                    android:progressTint="#F61807"
                    android:filterTouchesWhenObscured="false"
                    android:stepSize="1" />
            </TableRow>
            <TableRow
                android:layout_marginTop="10dp">
            <TextView
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:text="detail description "/>
            <EditText
                android:id="@+id/description"
                android:layout_height="50dp"
                android:layout_width="match_parent"
                android:layout_marginLeft="5dp"
                android:hint=" "
                android:layout_weight="1"/>
            </TableRow>
            <Button

                android:id="@+id/register"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:text="REGISTER"
                android:layout_marginTop="50dp"
                android:gravity="center"/>


        </TableLayout>
    </ScrollView>
    </androidx.core.widget.NestedScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>